{
    "name": "boleto",
    "version": "0.0.1",
    "author": "Allan Nubling <nubling@gmail.com>",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node-dev -r tsconfig-paths/register src/main/server.ts",
        "dev:docker": "ts-node-dev --inspect=0.0.0.0:7001 --poll -r tsconfig-paths/register src/main/server.ts",
        "debug": "ts-node-dev --inspect -r tsconfig-paths/register src/main/server.ts",
        "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files --no-copy-ignored",
        "start": "node ./build/main/server.js",
        "lint": "eslint --quiet --fix --ext .js,.ts && prettier -w --loglevel silent ./src && tsc --noEmit",
        "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --forceExit",
        "test:cov": "jest --collectCoverage",
        "test-verbose": "jest --passWithNoTests --runInBand",
        "test:watch": "jest --passWithNoTests --noStackTrace=false --runInBand --watch --no-cache",
        "test:unit": "npm test -- -c jest-unit-config.ts",
        "test:int": "npm test -- -c jest-integration-config.ts --forceExit",
        "postinstall": "node ./huskyPrepare.js"
    },
    "dependencies": {
        "body-parser": "^1.18.3",
        "cors": "^2.8.4",
        "dotenv": "^8.2.0",
        "express": "^4.16.3",
        "express-swagger-generator": "^1.1.17"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.4",
        "@babel/core": "^7.15.5",
        "@babel/node": "^7.15.4",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-proposal-decorators": "^7.15.8",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-typescript": "^7.15.0",
        "@commitlint/cli": "^13.2.0",
        "@commitlint/config-conventional": "^13.2.0",
        "@sentry/cli": "^1.71.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.1",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/node": "14.14.31",
        "@types/supertest": "^2.0.11",
        "@types/validator": "^13.6.3",
        "@types/yup": "^0.29.13",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-source-map-support": "^2.1.3",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-import-helpers": "^1.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.2",
        "jest": "^27.2.0",
        "jest-junit": "^12.3.0",
        "json2yaml": "^1.1.0",
        "lint-staged": "^11.1.2",
        "prettier": "^2.4.0",
        "supertest": "^6.1.6",
        "swagger2openapi": "^7.0.8",
        "ts-jest": "^27.0.5",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.11.0",
        "typescript": "^4.4.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
